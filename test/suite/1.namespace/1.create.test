systemctl start nonsense-netns@test.service

systemctl status nonsense-netns@test.service
systemctl status nonsense-test.slice
systemctl is-system-running

ip netns | grep -q '^nonsense:test$'

ip netns exec nonsense:test ip netns id | grep -q '^nonsense:test$'

systemctl stop nonsense-netns@test.service
systemctl is-system-running

! ip netns | grep -q '^nonsense:test$'

